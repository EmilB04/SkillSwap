{
  "name": "@redwoodjs/starter-standard",
  "version": "1.0.0",
  "description": "Build fast, server-driven webapps on Cloudflare with SSR, RSC, and realtime. ",
  "main": "src/worker.tsx",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "vite build",
    "dev": "vite dev",
    "dev:init": "rw-scripts dev-init",
    "preview": "vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "pnpm run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "pnpm run clean && pnpm run build && wrangler deploy",
    "migrate:dev": "wrangler d1 migrations apply DB --local",
    "migrate:prd": "wrangler d1 migrations apply DB --remote",
    "migrate:new": "drizzle-kit generate --config=./drizzle.config.ts",
    "seed": "pnpm run worker:run ./src/db/seed.ts",
    "check": "pnpm run generate && pnpm run types",
    "types": "tsc",
    "prepare": "vibe-rules install cursor",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "@prisma/adapter-d1": "~6.8.2",
    "@prisma/client": "~6.8.2",
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/server": "^13.1.1",
    "@tailwindcss/vite": "^4.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "expo": "^54.0.12",
    "nanoid": "^5.1.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rwsdk": "1.0.0-beta.30",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.7.4",
    "@cloudflare/workers-types": "^4.20250407.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "drizzle-kit": "^0.31.5",
    "jsdom": "^27.0.1",
    "prisma": "~6.8.2",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.8.3",
    "vibe-rules": "^0.2.31",
    "vite": "^6.2.6",
    "vitest": "^3.2.4",
    "wrangler": "~4.20.5"
  },
  "pnpm": {
    "overrides": {
      "react": "19.2.0",
      "react-dom": "19.2.0",
      "esbuild": ">=0.24.3"
    },
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "sharp",
      "workerd"
    ]
  }
}
